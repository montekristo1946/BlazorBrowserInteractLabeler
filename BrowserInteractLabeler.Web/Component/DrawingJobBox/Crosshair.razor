@inherits CrosshairModel






<line x1="@GetVerticalPoints().p1.X%" y1="@GetVerticalPoints().p1.Y%" x2="@GetVerticalPoints().p2.X%" y2="@GetVerticalPoints().p2.Y%" stroke="@ActiveLabelColor" stroke-width="@GetThicknessLine()" stroke-dasharray="4 2"> </line>
<line x1="@GetHorizontalPoint().p1.X%" y1="@GetHorizontalPoint().p1.Y%" x2="@GetHorizontalPoint().p2.X%" y2="@GetHorizontalPoint().p2.Y%" stroke="@ActiveLabelColor" stroke-width="@GetThicknessLine()" stroke-dasharray="4 2"> </line>

<line x1="@GetLinePoint().p1.X%" y1="@GetLinePoint().p1.Y%" x2="@GetLinePoint().p2.X%" y2="@GetLinePoint().p2.Y%" stroke="@ActiveLabelColor" stroke-width="@GetThicknessLine()" stroke-dasharray="4 2"> </line>
<line x1="@GetLinePointPolygon().p1.X%" y1="@GetLinePointPolygon().p1.Y%" x2="@GetLinePointPolygon().p2.X%" y2="@GetLinePointPolygon().p2.Y%" stroke="@ActiveLabelColor" stroke-width="@GetThicknessLine()" stroke-dasharray="4 2"> </line>
