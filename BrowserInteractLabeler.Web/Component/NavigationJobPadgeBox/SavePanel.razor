@inherits SavePanelModel

<div class="row m-0 p-0" id="save_panel">
    @* <div class="col-2"></div> *@
<div class="col-1"></div>
    <div class="col-2" style="">
        <button type="button" class="btn  bottom-shadow " data-mdb-toggle="tooltip" @onclick="ButtonSaveClick"
                data-mdb-placement="right" title="Save">
            <span role="img" class="anticon ">
                <img src="icons/001_save.svg" width="30" height="30"/>
            </span>
        </button>
    </div>
<div class="col-1"></div>
    <div class="col-2" style="">
        <button type="button" class="btn  bottom-shadow " data-mdb-toggle="tooltip" @onclick="ButtonUndoClick"
                data-mdb-placement="right" title="Undo">
            <span role="img" class="anticon ">
                <img src="icons/002_undo.svg" width="30" height="30"/>
            </span>
        </button>
    </div>
<div class="col-1"></div>
    <div class="col-2" style="">
        <button type="button" class="btn  bottom-shadow " data-mdb-toggle="tooltip" @onclick="ButtonRedoClick"
                data-mdb-placement="right" title="Redo">
            <span role="img" class="anticon ">
                <img src="icons/003_redo.svg" width="30" height="30"/>
            </span>
        </button>
    </div>
</div>